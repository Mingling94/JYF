{
  "manifest_version": 2,

  "name": "JYF",
  "description": "Judge Your Friends!",
  "version": "4.6",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Judge!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches":["https://www.facebook.com/*/photos*"],
      "js":["js/jquery-1.11.3.min.js", "js/photosContext.js"],
      "run_at": "document_end"
    },{
      "matches":["https://www.facebook.com/*"],
      "exclude_matches":["https://www.facebook.com/*/photos*"],
      "js":["js/jquery-1.11.3.min.js", "js/postContext.js"],
      "run_at": "document_end"
    }
  ]
}
